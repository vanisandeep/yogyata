<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>todo</Title>
    <Author>Arkadiusz Beer</Author>
    <Shortcut>todo</Shortcut>
    <Description>Markup snippet for a todo comment</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
      <Declarations>
      <Literal>
        <ID>comment</ID>
        <ToolTip>comment</ToolTip>
        <Default>comment</Default>
      </Literal>
    </Declarations>
    <Code Language="html"><![CDATA[<%-- todo: $comment$ --%>
	$end$]]></Code>
  </Snippet>
</CodeSnippet>