<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>test_setup_sub</Title>
			<Shortcut>test_setup_sub</Shortcut>
			<Description>Code snippet for test method</Description>
			<Author>Arkadiusz Beer</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
      <Imports>
        <Import>
          <Namespace>NUnit.Framework</Namespace>
        </Import>
      </Imports>
      <References>
        <Reference>
          <Assembly>nunit.framework.dlldll</Assembly>
        </Reference>
      </References>
			<Declarations>
			<Literal Editable="false">
				<ID>Test</ID>
				<Function>SimpleTypeName(global:: NUnit.Framework.SetUpAttribute)</Function>
			</Literal>
			<Literal >
				<ID>classname</ID>
				<ToolTip>Class name</ToolTip>
				<Function>ClassName()</Function>
				<Default>ClassNamePlaceholder</Default>
			</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[private $classname$ SubjectUnderTest; 
			
[$Test$]
public void  SetUp() 
	{
			SubjectUnderTest = new $classname$($selected$$end$);
	}]]></Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>